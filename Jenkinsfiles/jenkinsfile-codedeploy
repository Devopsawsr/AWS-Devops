pipeline {
    agent any
    stages {    
        stage('Running on MNP-Dev Environment') {
            steps{
                checkout scm
                echo "Removing files and folders"
                sh 'ls -al'
                sh 'rm -rf .git'
                echo "Deploying on MNP-DEV Environment by Code deploy"    
                step([$class: 'AWSCodeDeployPublisher',
                       applicationName:'mnptech-dev-app',
                       awsAccessKey: '', awsSecretKey: '', deploymentConfig: 'CodeDeployDefault.OneAtATime', deploymentGroupAppspec: false, deploymentGroupName: 'mnptech-dev-dg', excludes: '', iamRoleArn: '', includes: '**', proxyHost: '', proxyPort: 0, region: 'us-east-1', s3bucket: 'mnptech-app-artif ', s3prefix: 'mnptech-dev-app', subdirectory: '', versionFileName: '', waitForCompletion: false?]
                    )    
            }    
        }                
    }
}
